@model IEnumerable<BS.DMO.ViewModels.Transport.DELIVERY_AGENT_VM>
@{
    ViewData["Title"] = "Delivery Agent";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="card card-primary card-outline">
    <div class="card-header">
        <h3 class="card-title">Find your @ViewData["Title"]</h3>
        <div class="card-tools">
            <a class="btn btn-primary btn-xs" asp-action="Create"><i class="fa fa-plus"></i> Create New</a>
        </div>
    </div>

    <div class="card-body">
        <table class="table table-hover table-sm">
            <thead>
                <tr>
                    <th>Agent Type</th>
                    <th>Agent No</th>
                    <th>Agent Name</th>
                    <th>Contact</th>
                    <th>Address</th>
                    <th>Load Capacity</th>
                    <th>KPL</th>
                    <th>Status</th>
                    <th>#</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.DELIVERY_AGENT_TYPE)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.AGENT_NO)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.AGENT_NAME)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.AGENT_CONTACT)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.AGENT_ADDRESS)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.LOAD_CAPACITY)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.KPL)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.IS_ACTIVE)
                        </td>
                        <td>
                            <a class="btn btn-primary btn-xs" asp-action="Edit" asp-route-id="@item.ID"><i class="fa fa-edit"></i> Edit</a>
                            <button type="button" class="btn btn-danger btn-xs delete-confirm" data-url="/Transport/DeliveryAgent/Delete/@item.ID"><i class="fa fa-trash"></i> Delete</button>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>